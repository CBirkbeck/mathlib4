    ‚àÉ C : ‚Ñù‚â•0, ‚àÄ (u : E ‚Üí F') (hu : ContDiff ‚Ñù 1 u) (h2u : u.support ‚äÜ s),
    snorm u q Œº ‚â§ C * snorm (fderiv ‚Ñù u) p Œº := by
  let p' : ‚Ñù‚â•0 := (p‚Åª¬π - (finrank ‚Ñù E : ‚Ñù‚â•0)‚Åª¬π)‚Åª¬π
  have hp' : p'‚Åª¬π = p‚Åª¬π - (finrank ‚Ñù E : ‚Ñù)‚Åª¬π := by
    rw [inv_inv, NNReal.coe_sub]
    ¬∑ simp
    ¬∑ gcongr
  have : (q : ‚Ñù‚â•0‚àû) ‚â§ p' := by
    have H : (p':‚Ñù)‚Åª¬π ‚â§ (‚Üëq)‚Åª¬π := trans hp' hpq
    norm_cast at H ‚ä¢
    rwa [inv_le_inv] at H
    ¬∑ dsimp
      have : 0 < p‚Åª¬π - (finrank ‚Ñù E : ‚Ñù‚â•0)‚Åª¬π := by
        simp only [tsub_pos_iff_lt]
        gcongr
      positivity
