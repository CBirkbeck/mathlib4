    rw [power_of_alpha_plus_one X n a (a + n) b rfl (by rw [‚Üê hn, Nat.cast_add, Nat.cast_one])]
    simp only [Functor.comp_obj, Iso.app_hom, Iso.symm_hom, assoc]
